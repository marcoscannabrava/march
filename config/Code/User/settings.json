{ "workbench.colorTheme": "Tokyo Night",
    "keyboard.dispatch": "keyCode",
    // Editor & Theme Customizations
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.inlineSuggest.enabled": true,
    "editor.bracketPairColorization.enabled": false,
    "explorer.confirmDelete": true,
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreview": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "statusBar.background": "#1E1E1E",
            "statusBar.foreground": "#bbbbbb",
            "statusBar.debuggingForeground": "#1E1E1E",
            "sideBar.background": "#1E1E1E",
            "sideBar.foreground": "#bbbbbb",
            "activityBar.background": "#1E1E1E",
            "editorGroupHeader.tabsBackground": "#1E1E1E",
            "editorLineNumber.foreground": "#5e5e5e",
            "tab.inactiveBackground": "#1E1E1E",
            "tab.activeBorder": "#e58520"
        }
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.section.markdown",
                    "punctuation.definition.heading.markdown"
                ],
                "settings": {
                    "foreground": "#60b600"
                }
            },
            {
                "scope": [
                    "string.other.link.title.markdown"
                ],
                "settings": {
                    "foreground": "#008bcc"
                }
            },
            {
                "scope": [
                    "markup.underline.link.markdown",
                    "punctuation.definition.metadata.markdown"
                ],
                "settings": {
                    "foreground": "#55462d",
                    "fontStyle": "italic underline",
                }
            }
        ]
    },
    // Language-specific settings
    "emmet.includeLanguages": {
        "html.erb": "html",
        "erb": "html"
    },
    "emmet.excludeLanguages": [
        "markdown"
    ],
    "emmet.triggerExpansionOnTab": true,
    "emmet.showSuggestionsAsSnippets": false,
    "eslint.format.enable": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features",
        "editor.tabSize": 2
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
        "editor.tabSize": 2
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "markdown.extension.print.theme": "dark",
    "python.defaultInterpreterPath": "python3",
    "python.analysis.typeCheckingMode": "basic",
    "[python]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "jupyter.alwaysTrustNotebooks": true,
    // Global Extension Settings
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.enabled": false,
    "gitlens.menus": {
        "editor": {
            "blame": false,
            "clipboard": true,
            "compare": true,
            "history": false,
            "remote": false
        },
        "editorGroup": false,
        "editorTab": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "explorer": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": true
        },
        "scm": {
            "authors": true
        },
        "scmGroupInline": {
            "stash": true
        },
        "scmGroup": {
            "compare": true,
            "openClose": true,
            "stash": true
        },
        "scmItem": {
            "clipboard": true,
            "compare": true,
            "history": true,
            "remote": false,
            "stash": true
        }
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "telemetry.enableTelemetry": false,
    "files.exclude": {
        ".expo*": true,
        "**/.next": true,
        "**/.yarn": true,
        "**/bower_components": true,
        "**/node_modules": true,
        "**/tmp": true,
        "**/venv": true
    },
    "files.watcherExclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "**/tmp/**": true,
        "**/venv/**": true
    },
    "search.exclude": {
        "**/tmp": true
    },
    "security.workspace.trust.untrustedFiles": "newWindow",
    "window.menuBarVisibility": "compact",
    "liveServer.settings.donotShowInfoMsg": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": false,
        "scminput": false,
        "yaml": false
    },
    "gitlens.statusBar.enabled": false,
    "debug.allowBreakpointsEverywhere": true,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.check.command": "clippy",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "'Ubuntu Mono', 'monospace', monospace",
    "react-native-tools.showUserTips": false,
    "git.openRepositoryInParentFolders": "never",
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cmake.configureOnOpen": true,
    "apiKey": "<OPENAI_API_KEY_HERE>",
    "highlight.regexes": {
        "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
            "filterFileRegex": ".*(?<!CHANGELOG.md)$",
            "decorations": [
                {
                    "overviewRulerColor": "#eccc50",
                    "backgroundColor": "#eccc50",
                    "color": "#1f1f1f",
                    "fontWeight": "bold"
                },
                {
                    "backgroundColor": "#eccc50",
                    "color": "#1f1f1f"
                }
            ]
        },
        "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIXME|FIX|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
            "filterFileRegex": ".*(?<!CHANGELOG.md)$",
            "decorations": [
                {
                    "overviewRulerColor": "#c05050",
                    "backgroundColor": "#c05050",
                    "color": "#1f1f1f",
                    "fontWeight": "bold"
                },
                {
                    "backgroundColor": "#c05050",
                    "color": "#1f1f1f"
                }
            ]
        },
        "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|WIP|REFACTOR)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
            "filterFileRegex": ".*(?<!CHANGELOG.md)$",
            "decorations": [
                {
                    "overviewRulerColor": "#00ccff",
                    "backgroundColor": "#00ccff",
                    "color": "#1f1f1f",
                    "fontWeight": "bold"
                },
                {
                    "backgroundColor": "#00ccff",
                    "color": "#1f1f1f"
                }
            ]
        },
        "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEAS?)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
            "filterFileRegex": ".*(?<!CHANGELOG.md)$",
            "decorations": [
                {
                    "overviewRulerColor": "#da4dda",
                    "backgroundColor": "#cc00cc",
                    "color": "#ffffff",
                    "fontWeight": "bold"
                },
                {
                    "backgroundColor": "#cc00cc",
                    "color": "#ffffff"
                }
            ]
        },
        "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:NOTES?)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
            "filterFileRegex": ".*(?<!CHANGELOG.md)$",
            "decorations": [
                {
                    "overviewRulerColor": "#00cc00",
                    "backgroundColor": "#00cc00",
                    "color": "#1f1f1f",
                    "fontWeight": "bold"
                },
                {
                    "backgroundColor": "#00cc00",
                    "color": "#1f1f1f"
                }
            ]
        }
    },
    "go.toolsManagement.autoUpdate": true,
    "go.lintTool": "golangci-lint",
    "githubPullRequests.pullBranch": "never",
    "inlineChat.showDiff": false,
    "remote.autoForwardPortsSource": "hybrid",
    "markdown.styles": [
        "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    ],
    "editor.tabSize": 2,
    "eslint.codeActionsOnSave.rules": null,
    "githubPullRequests.terminalLinksHandler": "github",
    "gitlens.ai.experimental.provider": "openai",
    "gitlens.ai.experimental.openai.model": "gpt-4-turbo-preview",
    "terminal.integrated.inheritEnv": false,
    "cSpell.userWords": [
        "CLASSNAME",
        "euclidian",
        "headlessui",
        "heroicons",
        "Ikigai",
        "ILIKE",
        "oews",
        "onet",
        "outro",
        "pgvector",
        "supabase"
    ],
    "prisma.showPrismaDataPlatformNotification": false,
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "[graphql]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "gitlens.views.searchAndCompare.files.icon": "status",
    "window.confirmSaveUntitledWorkspace": false,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "git.blame.editorDecoration.enabled": false,
    "jest.runMode": "on-demand",
    "python.analysis.extraPaths": [
        "vpoc/apps" // for valence django repo
    ],
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "workbench.iconTheme": "Monokai Pro (Filter Ristretto) Icons",
}